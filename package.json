{
  "name": "glvoice",
  "version": "3.0.1-beta",
  "description": "Library for retrieving model objects from a JSON-API, with a fluent syntax inspired by Laravel Eloquent.",
  "keywords": [
    "coloquent",
    "eloquent",
    "json-api",
    "json",
    "api",
    "json-api client",
    "api client",
    "json-api-client",
    "model",
    "orm",
    "mvc"
  ],
  "author": "David Duwaer",
  "license": "MIT",
  "homepage": "https://github.com/DavidDuwaer/coloquent#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DavidDuwaer/coloquent.git"
  },
  "bugs": {
    "url": "https://github.com/DavidDuwaer/coloquent/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "dist": "dist"
  },
  "scripts": {
    "clear": "rimraf dist",
    "dev": "npm run clear && tsc --watch --sourceMap",
    "build": "npm run clear && tsc",
    "build-dev": "npm run clear && tsc --sourceMap",
    "dist": "npm install && npm publish",
    "format": "prettier --write ."
  },
  "dependencies": {
    "error-stack-parser": "^2.0.6",
    "php-date-formatter": "^1.3.4"
  },
  "devDependencies": {
    "@tanstack/react-query": "^5.63.0",
    "@types/chai": "^4.2.18",
    "@types/chai-as-promised": "^7.1.4",
    "@types/mocha": "^5.2.7",
    "@types/moxios": "^0.4.9",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "mocha": "^6.2.2",
    "moxios": "^0.4.0",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "rimraf": "^3.0.0",
    "ts-node": "^8.4.1",
    "typescript": "^4.5.4"
  }
}
